{
    "name": "luigimannoni/wordpress-docker-starter",
    "description": "Main wordpress website for anewstandard.com",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Luigi Mannoni",
            "email": "hello@luigimannoni.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
